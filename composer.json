{
    "name": "jobmetric/laravel-translation",
    "description": "This is a package for translating the contents of different Laravel projects.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "translation",
        "translatable",
        "model"
    ],
    "homepage": "https://doc.jobmetric.net/package/laravel-translation",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "ext-json": "*",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.18.5",
        "jobmetric/laravel-metadata": "^2.2",
        "jobmetric/laravel-language": "^1.8",
        "jobmetric/laravel-custom-field": "^1.7"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\Translation\\": "src/",
            "JobMetric\\Translation\\Factories\\": "database/factories/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --colors=always",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\Translation\\TranslationServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-translation/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-translation"
    }
}
